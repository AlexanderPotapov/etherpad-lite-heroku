{
  "name": "etherpad-lite-heroku",
  "version": "1.0.0",
  "description": "Deploy Etherpad-Lite on Heroku.",
  "scripts": {
    "heroku-prebuild": "node herokuPrebuild.js",
    "start-dev": "npm run heroku-prebuild && NODE_ENV=development node start.js && npm restore-original-package",
    "restore-original-package": "mv package.json.backup package.json"
  },
  "cacheDirectories": [
    "node_modules",
    "etherpad_lite/node_modules"
  ],
  "engines": {
    "node": "6.13.1",
    "npm": "3.10.10"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:citizenos/etherpad-lite-heroku.git"
  },
  "dependencies": {
    "kexec": "2.0.2"
  }
}