{
  "name": "etherpad-lite-heroku",
  "version": "1.0.3",
  "description": "Deploy Etherpad-Lite on Heroku. Whole configuration can be defined in env or config/local.json.",
  "scripts": {
    "start-dev": "npm install && NODE_ENV=development NODE_TLS_REJECT_UNAUTHORIZED=0 node start.js",
    "build": "cd etherpad-lite/src && npm install"
  },
  "cacheDirectories": [
    "node_modules",
    "etherpad-lite/src/node_modules"
  ],
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:citizenos/etherpad-lite-heroku.git"
  },
  "dependencies": {
    "ep_align": "0.0.24",
    "ep_auth_citizenos": "git+https://github.com/citizenos/ep_auth_citizenos.git#1e28635b7d0275d00a08908466728622f522e210",
    "ep_authorship_toggle": "0.0.3",
    "ep_comments_page": "git+https://github.com/citizenos/ep_comments.git#928260730369458d38d89a7c43844196b728f3ce",
    "ep_embed_floating_toolbar": "0.0.3",
    "ep_font_color": "git+https://github.com/citizenos/ep_font_color.git",
    "ep_font_size": "git+https://github.com/citizenos/ep_font_size.git#0079fae7ba226f83a81905a3638093cbe5a74b34",
    "ep_foot_note": "git+https://github.com/citizenos/ep_foot_note.git#809b403a1e7f819849a35198b18407befd100f41",
