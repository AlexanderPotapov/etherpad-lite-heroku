    "node_modules",
    "etherpad-lite/src/node_modules",
    "etherpad-lite/node_modules",
    "src/node_modules"
  ],
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:citizenos/etherpad-lite-heroku.git"
  },
