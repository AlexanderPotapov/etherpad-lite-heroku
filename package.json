    "ep_page_view": "^0.5.24",
    "ep_resize": "git+https://github.com/citizenos/ep_resize.git#87c057ef8bdaecbba8b9c6168baf32ba67b65f0b",
    "ep_themes_ext": "git+https://github.com/ilmartyrk/ep_themes_ext.git#9adb6e3d05cce504659d7ec364afb2d671cb4c16",
    "ep_title_limit": "git+https://github.com/citizenos/ep_title_limit.git",
