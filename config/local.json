  "ep_image_upload": {
    "___version": "1.0.0",
    "s3": {
      "accessKeyId": "AWS_ACCESS_KEY_ID",
      "secretAccessKey": "AWS_SECRET_ACCESS_KEY",
      "region": "AWS_REGION",
      "bucket": "AWS_S3_BUCKET_NAME",
      "baseFolder": "ep"
    },
    "fileTypes": [
      "jpeg",
      "jpg",
      "bmp",
      "gif",
      "png"
    ],
    "maxFileSize": 5000000
  }
